<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!-- Note: For Android please see also Platforms\Android\Resources\values\colors.xml -->

    <Color x:Key="Primary">#512BD4</Color>
    <Color x:Key="PrimaryDark">#ac99ea</Color>
    <Color x:Key="PrimaryDarkText">#242424</Color>
    <Color x:Key="Secondary">#DFD8F7</Color>
    <Color x:Key="SecondaryDarkText">#9880e5</Color>
    <Color x:Key="Tertiary">#2B0B98</Color>

    <Color x:Key="White">White</Color>
    <Color x:Key="Black">Black</Color>
    <Color x:Key="Magenta">#D600AA</Color>
    <Color x:Key="MidnightBlue">#190649</Color>
    <Color x:Key="OffBlack">#1f1f1f</Color>

    <Color x:Key="Gray100">#E1E1E1</Color>
    <Color x:Key="Gray200">#C8C8C8</Color>
    <Color x:Key="Gray300">#ACACAC</Color>
    <Color x:Key="Gray400">#919191</Color>
    <Color x:Key="Gray500">#6E6E6E</Color>
    <Color x:Key="Gray600">#404040</Color>
    <Color x:Key="Gray700">#333333</Color>
    <Color x:Key="Gray800">#292929</Color>
    <Color x:Key="Gray900">#212121</Color>
    <Color x:Key="Gray950">#141414</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}"/>
    <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource Tertiary}"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}"/>

    <SolidColorBrush x:Key="Gray100Brush" Color="{StaticResource Gray100}"/>
    <SolidColorBrush x:Key="Gray200Brush" Color="{StaticResource Gray200}"/>
    <SolidColorBrush x:Key="Gray300Brush" Color="{StaticResource Gray300}"/>
    <SolidColorBrush x:Key="Gray400Brush" Color="{StaticResource Gray400}"/>
    <SolidColorBrush x:Key="Gray500Brush" Color="{StaticResource Gray500}"/>
    <SolidColorBrush x:Key="Gray600Brush" Color="{StaticResource Gray600}"/>
    <SolidColorBrush x:Key="Gray900Brush" Color="{StaticResource Gray900}"/>
    <SolidColorBrush x:Key="Gray950Brush" Color="{StaticResource Gray950}"/>

    <!-- JAZER Design System Colors -->
    
    <!-- Grays -->
    <Color x:Key="Gray50">#F9FAFB</Color>
    <Color x:Key="Gray100Jazer">#F3F4F6</Color>
    <Color x:Key="Gray200Jazer">#E5E7EB</Color>
    <Color x:Key="Gray300Jazer">#D1D5DB</Color>
    <Color x:Key="Gray400Jazer">#9CA3AF</Color>
    <Color x:Key="Gray500Jazer">#6B7280</Color>
    <Color x:Key="Gray600Jazer">#4B5563</Color>
    <Color x:Key="Gray700Jazer">#374151</Color>
    <Color x:Key="Gray800Jazer">#1F2937</Color>
    <Color x:Key="Gray900Jazer">#111827</Color>

    <!-- Blues -->
    <Color x:Key="Blue50">#EFF6FF</Color>
    <Color x:Key="Blue100">#DBEAFE</Color>
    <Color x:Key="Blue200">#BFDBFE</Color>
    <Color x:Key="Blue300">#93C5FD</Color>
    <Color x:Key="Blue400">#60A5FA</Color>
    <Color x:Key="Blue500">#3B82F6</Color>
    <Color x:Key="Blue600">#2563EB</Color>
    <Color x:Key="Blue700">#1D4ED8</Color>
    <Color x:Key="Blue800">#1E40AF</Color>
    <Color x:Key="Blue900">#1E3A8A</Color>

    <!-- Greens -->
    <Color x:Key="Green50">#F0FDF4</Color>
    <Color x:Key="Green100">#DCFCE7</Color>
    <Color x:Key="Green200">#BBF7D0</Color>
    <Color x:Key="Green300">#86EFAC</Color>
    <Color x:Key="Green400">#4ADE80</Color>
    <Color x:Key="Green500">#10B981</Color>
    <Color x:Key="Green600">#059669</Color>
    <Color x:Key="Green700">#047857</Color>
    <Color x:Key="Green800">#065F46</Color>

    <!-- Reds -->
    <Color x:Key="Red50">#FEF2F2</Color>
    <Color x:Key="Red100">#FEE2E2</Color>
    <Color x:Key="Red200">#FECACA</Color>
    <Color x:Key="Red300">#FCA5A5</Color>
    <Color x:Key="Red400">#F87171</Color>
    <Color x:Key="Red500">#EF4444</Color>
    <Color x:Key="Red600">#DC2626</Color>
    <Color x:Key="Red700">#B91C1C</Color>
    <Color x:Key="Red800">#991B1B</Color>

    <!-- Yellows/Oranges -->
    <Color x:Key="Yellow50">#FFFBEB</Color>
    <Color x:Key="Yellow100">#FEF3C7</Color>
    <Color x:Key="Yellow200">#FDE68A</Color>
    <Color x:Key="Yellow300">#FDE047</Color>
    <Color x:Key="Orange400">#FB923C</Color>
    <Color x:Key="Orange500">#F59E0B</Color>
    <Color x:Key="Orange600">#D97706</Color>
    <Color x:Key="Yellow600">#CA8A04</Color>

    <!-- Purples -->
    <Color x:Key="Purple50">#F3E8FF</Color>
    <Color x:Key="Purple100">#E9D5FF</Color>
    <Color x:Key="Purple400">#A78BFA</Color>
    <Color x:Key="Purple500">#8B5CF6</Color>
    <Color x:Key="Purple600">#7C3AED</Color>
    <Color x:Key="Purple700">#6B21A8</Color>

    <!-- Brushes for JAZER colors -->
    <SolidColorBrush x:Key="Gray50Brush" Color="{StaticResource Gray50}"/>
    <SolidColorBrush x:Key="Blue500Brush" Color="{StaticResource Blue500}"/>
    <SolidColorBrush x:Key="Blue600Brush" Color="{StaticResource Blue600}"/>
    <SolidColorBrush x:Key="Green500Brush" Color="{StaticResource Green500}"/>
    <SolidColorBrush x:Key="Red500Brush" Color="{StaticResource Red500}"/>
    <SolidColorBrush x:Key="Orange500Brush" Color="{StaticResource Orange500}"/>
    <SolidColorBrush x:Key="Purple500Brush" Color="{StaticResource Purple500}"/>
</ResourceDictionary>